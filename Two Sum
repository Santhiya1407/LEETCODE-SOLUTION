class Solution {
    public int[] twoSum(int[] nums, int target) {
        int ans=0;
        int first=-1;
        int last=-1;
        for(int i=0;i<nums.length;i++){
            for(int j=i+1;j<nums.length;j++){
                ans=nums[i]+nums[j];
            if(ans==target){
               first=i;
               last=j;
             return new int[]{first,last};

            }
              
            }
           
        }
        return new int[]{first,last};
    }
}
